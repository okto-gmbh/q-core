    },
    "scripts": {
        "build": "yarn build:esm && yarn build:cjs",
        "build:cjs": "tsc --project ./tsconfig.json --skipLibCheck --module commonjs",
        "build:esm": "tsc --project ./tsconfig.json --skipLibCheck --outDir lib/esm",
        "build:watch": "tsc --project ./tsconfig.json --watch --module commonjs",
        "prebuild": "rimraf -rf ./lib"
    },
    "engines": {
        "node": ">=16.15.0"
    },
    "files": [
        "/lib"
    ],
    "peerDependencies": {
        "@blitzjs/auth": "^2.0.0-beta.20",
