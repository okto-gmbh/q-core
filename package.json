        "@emotion/styled": "^11.10.4",
        "@mui/icons-material": "^5.10.3",
        "@mui/material": "^5.10.5",
        "@mui/x-date-pickers-pro": "^5.0.1",
        "autosuggest-highlight": "^3.3.4",
        "blitz": "^2.0.0-beta.20",
        "color": "^4.2.3",
        "firebase": "^9.12.1",
        "firebase-admin": "^11.4.0",
        "graphql-request": "^5.1.0",
        "lodash.merge": "^4.6.2",
        "lodash.throttle": "^4.1.1",
        "next": "^12.2.5",
